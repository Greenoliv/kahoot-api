{
  "name": "kahoot-api",
  "version": "0.7.0",
  "description": "An API for the Kahoot quiz service.",
  "main": "dist/index.js",
  "browser": "lib/index.js",
  "scripts": {
    "build-browser": "cross-env BABEL_ENV=browser babel ./src --out-dir ./lib --source-maps --copy-files",
    "build-node": "babel ./src --out-dir ./dist --source-maps --copy-files",
    "build": "npm run build-node && npm run build-browser",
    "test": "mocha --bail --no-timeouts --require @babel/register --require @babel/polyfill",
    "prepublish": "npm run build"
  },
  "repository": "https://github.com/idiidk/kahoot-api",
  "author": "idiidk",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/node": "^7.2.2",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/register": "^7.4.4",
    "browserify": "^16.2.3",
    "chai": "^4.2.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.2",
    "mocha": "^6.1.4"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "cometd": "^4.0.3",
    "cometd-nodejs-client": "^1.0.2",
    "core-js": "2",
    "tiny-emitter": "^2.1.0"
  }
}
